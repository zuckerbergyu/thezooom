{
  "name": "nextjs-with-typescript",
  "version": "5.0.0",
  "private": true,
  "scripts": {
    "dev:staging": "cross-env APP_ENV=DEV next dev -p 8080",
    "dev:prod": "cross-env APP_ENV=PROD next dev",
    "dev": "npm run dev:staging",
    "build": "cross-env NODE_ENV=production APP_ENV=PROD next build", 
    "exports":"cross-env NODE_ENV=production APP_ENV=PROD next build && next export -o ../../src/main/resources/static", 
    "start": "cross-env NODE_ENV=production APP_ENV=PROD next start",
    "lint": "next lint",
    "post-update": "echo \"codesandbox preview only, need an update\" && yarn upgrade --latest"
  },
  "dependencies": {
    "@emotion/cache": "^11.9.3",
    "@emotion/react": "^11.9.3",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.9.3",
    "@mui/icons-material": "^5.8.4",
    "@mui/lab": "^5.0.0-alpha.92",
    "@mui/material": "^5.9.1",
    "@types/smoothscroll-polyfill": "^0.3.1",
    "axios": "^1.1.2",
    "dotenv": "^16.0.1",
    "iframe-resizer-react": "^1.1.0",
    "next": "^12.2.2",
    "next-seo": "^5.4.0",
    "react": "^18.2.0",
    "react-device-detect": "^2.2.2",
    "react-dom": "^18.2.0",
    "react-query": "^3.39.2",
    "swiper": "^8.4.4",
    "smoothscroll-polyfill": "^0.4.4"
  },
  "devDependencies": {
    "@types/eslint": "^8.4.3",
    "@types/jest": "^29.1.2",
    "@types/node": "^18.0.6",
    "@types/prettier": "^2.7.1",
    "@types/react": "^18.0.15",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "eslint": "^8.26.0",
    "eslint-config-next": "^12.3.1",
    "eslint-plugin-react": "^7.31.10",
    "jest": "^29.1.2",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4"
  }
}
